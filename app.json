      "expo-web-browser",
      "react-native-compressor",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f6a25cc0-4347-4470-9cfc-d90bbca09220"
