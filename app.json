{
  "expo": {
    "name": "geoteller",
    "slug": "geoteller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geoteller",
      "associatedDomains": [
        "applinks:https://deycthvditigtcwtvals.supabase.co"
      ],
      "config": {
        "googleMapsApiKey": "AIzaSyArQQgKA4uz4bwoz4l2THPNVCOlususLFc",
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.geoteller"
            ]
          }
        ],
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to discover nearby stories, provide directions, and offer relevant content.",
        "NSCameraUsageDescription": "This app needs access to your camera to allow you to take photos and videos for posts and profile pictures.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select images and videos for posts and profile pictures.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio for video posts and voice notes.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to your photo library to save photos and videos.",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "comgooglemaps-x-callback",
          "comgooglemapsurl",
          "com.googleusercontent.apps.*"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to provide navigation and location-based features.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to provide navigation and location-based features.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.geoteller",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyArQQgKA4uz4bwoz4l2THPNVCOlususLFc"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#2c3e50"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "backgroundColor": "#2c3e50",
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      "expo-image-picker",
      "expo-location",
      [
